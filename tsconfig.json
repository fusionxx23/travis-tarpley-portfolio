
{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@libs/*": ["src/libs/*"],
      "@layouts/*": ["src/layouts/*"],
      "@data/*": ["src/data/*"],
    }
  }
}
